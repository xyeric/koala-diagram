{
  "name": "koala-diagram",
  "productName": "Koala Diagram",
  "version": "0.0.2",
  "description": "A diagram tool for programmer",
  "main": "./dist/main.js",
  "scripts": {
    "build:darwin": "cross-env BUILD_TARGET=darwin node .bin/build.js",
    "build:builder:darwin": "cross-env BUILD_TARGET=darwin electron-builder --config .bin/builder.config.json",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .bin/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .bin/webpack.renderer.config.js",
    "dev": "node .bin/dev.js"
  },
  "repository": "https://github.com/xyeric/koala-diagram",
  "keywords": [
    "Electron",
    "koala",
    "diagram",
    "Koala Diagram",
    "typescript"
  ],
  "author": "GitHub",
  "license": "MIT",
  "devDependencies": {
    "@rematch/core": "^1.3.0",
    "@types/mermaid": "^8.2.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.6",
    "@types/react-transition-group": "^4.2.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cfonts": "^2.4.8",
    "chalk": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "css-modules-typescript-loader": "^4.0.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.2.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "lodash.merge": ">=4.6.2",
    "mermaid": "^8.4.6",
    "monaco-editor": "^0.19.3",
    "monaco-editor-webpack-plugin": "^1.8.2",
    "multispinner": "^0.2.1",
    "node-sass": "^4.13.1",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-transition-group": "^4.3.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "tslint": "^5.19.0",
    "typescript": "^3.6.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {},
  "config": {
    "electron_mirror": "https://npm.taobao.org/mirrors/electron/",
    "electron_custom_dir": "7.1.10"
  }
}
